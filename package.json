{
  "name": "account-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered social media account management system",
  "main": "server.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "reload": "pm2 reload ecosystem.config.js",
    "logs": "pm2 logs",
    "monitor": "pm2 monit",
    "build": "vite build",
    "dev": "concurrently --kill-others --success first --names \"MAIN,RAG,PROXY,FRONTEND\" \"npm run dev:main\" \"npm run dev:rag\" \"npm run dev:proxy\" \"npm run dev:frontend\"",
    "dev:main": "MAIN_SERVER_PORT=3000 node server/server.js",
    "dev:rag": "RAG_SERVER_PORT=3001 node rag-server.js",
    "dev:proxy": "PROXY_SERVER_PORT=3002 node server.js",
    "dev:frontend": "vite",
    "production:setup": "npm install --production && npm run build && mkdir -p logs && pm2 start ecosystem.config.js --env production",
    "production:status": "pm2 status",
    "production:logs": "pm2 logs --lines 50",
    "production:restart": "pm2 restart all",
    "production:stop": "pm2 stop all",
    "health": "curl -f http://localhost:3000/health && curl -f http://localhost:3001/health && curl -f http://localhost:3002/health",
    "test": "jest --testPathPatterns=src/tests --verbose",
    "test:watch": "jest --testPathPattern=src/tests --watch",
    "test:backend": "node src/tests/backendUsageSync.test.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cleanup-ports": "./cleanup-ports.sh",
    "cleanup": "./cleanup-ports.sh",
    "dev:clean": "./cleanup-ports.sh && npm run dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/genai": "^1.0.0",
    "@langchain/community": "^0.0.20",
    "@langchain/openai": "^0.0.14",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.1",
    "@types/axios": "^0.14.4",
    "@types/react-datepicker": "^4.19.6",
    "@types/three": "^0.177.0",
    "aws-sdk": "^2.1234.0",
    "axios": "^1.6.0",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.4.9",
    "chromadb": "^1.5.5",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-type": "^20.5.0",
    "firebase": "^11.6.1",
    "form-data": "^4.0.4",
    "framer-motion": "^12.7.2",
    "glob": "^11.0.2",
    "http-proxy-middleware": "^3.0.5",
    "jpeg-js": "^0.4.4",
    "konva": "^9.3.20",
    "langchain": "^0.1.37",
    "lucide-react": "^0.526.0",
    "multer": "^1.4.5-lts.1",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.104.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.1",
    "react-icons": "^5.5.0",
    "react-konva": "^19.0.3",
    "react-router-dom": "^6.30.1",
    "react-slick": "^0.30.3",
    "sharp": "^0.32.6",
    "slick-carousel": "^1.8.1",
    "streamifier": "^0.1.1",
    "three": "^0.177.0",
    "tui-image-editor": "^3.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^5.4.19"
  },
  "keywords": [
    "social-media",
    "ai",
    "automation",
    "content-management"
  ],
  "author": "Account Manager Team",
  "license": "MIT"
}
