{
  "name": "account-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered social media account management system",
  "main": "server.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "reload": "pm2 reload ecosystem.config.js",
    "logs": "pm2 logs",
    "monitor": "pm2 monit",
    "build": "vite build",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:rag\" \"npm run dev:proxy\" \"npm run dev:frontend\"",
    "dev:main": "node server/server.js",
    "dev:rag": "node rag-server.js",
    "dev:proxy": "node server.js",
    "dev:frontend": "vite",
    "production:setup": "npm install --production && npm run build && mkdir -p logs && pm2 start ecosystem.config.js --env production",
    "production:status": "pm2 status",
    "production:logs": "pm2 logs --lines 50",
    "production:restart": "pm2 restart all",
    "production:stop": "pm2 stop all",
    "health": "curl -f http://localhost:3000/health && curl -f http://localhost:3001/health && curl -f http://localhost:3002/health",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cleanup-ports": "lsof -ti:3001,3002,5173 | xargs kill -9 || true"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@langchain/community": "^0.0.20",
    "@langchain/openai": "^0.0.14",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.1",
    "@types/axios": "^0.14.4",
    "@types/react-datepicker": "^4.19.6",
    "@types/three": "^0.177.0",
    "aws-sdk": "^2.1234.0",
    "axios": "^1.6.0",
    "chart.js": "^4.4.9",
    "chromadb": "^1.5.5",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-type": "^20.5.0",
    "firebase": "^11.6.1",
    "framer-motion": "^12.7.2",
    "glob": "^11.0.2",
    "http-proxy-middleware": "^3.0.5",
    "jpeg-js": "^0.4.4",
    "konva": "^9.3.20",
    "langchain": "^0.1.37",
    "multer": "^1.4.5-lts.1",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.0",
    "openai": "^4.104.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "react-icons": "^5.5.0",
    "react-konva": "^19.0.3",
    "react-router-dom": "^7.5.3",
    "react-slick": "^0.30.3",
    "sharp": "^0.32.6",
    "slick-carousel": "^1.8.1",
    "streamifier": "^0.1.1",
    "three": "^0.177.0",
    "tui-image-editor": "^3.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "terser": "^5.43.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "keywords": [
    "social-media",
    "ai",
    "automation",
    "content-management"
  ],
  "author": "Account Manager Team",
  "license": "MIT"
}
